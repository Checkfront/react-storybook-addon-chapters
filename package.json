{
  "name": "react-storybook-addon-chapters",
  "version": "1.1.2",
  "description": "React Storybook Chapters addon allows showcasing of multiple components within a story by breaking it down into smaller categories (chapters) and subcategories (sections) for more organizational goodness.",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonellis/react-storybook-addon-chapters.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9011",
    "my-storybook": "start-storybook -p 9011 -c .my-storybook",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.2.12",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^0.14.7 || ^15.0.0",
    "sinon": "^1.17.6"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "@storybook/react": "^3.2.12",
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "babel-runtime": "^6.5.0",
    "markdown-to-react-components": "^0.2.4",
    "react-addons-create-fragment": "^15.3.2"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
